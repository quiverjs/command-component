  "name": "quiver-command-component",
  "desription": "Quiver components that wrap around command line",
  "version": "1.2.0",
  "homepage": "https://github.com/quiverjs/quiver-command-component",
  "repository": {
    "url": "git@github.com:quiverjs/quiver-command-component.git"
  },
  "dependencies": {
    "temp": "~0.8.1",
    "quiver-core": "~1.1.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "mocha": "^2.0.1",
    "chai-as-promised": "^4.1.1"
  },
  "bugs": {
    "url": "https://github.com/quiverjs/quiver-command-component/issues"
  },
  "licenses": {
    "type": "MIT",
    "url": "https://raw.github.com/quiverjs/license/master/LICENSE"
  },
  "main": "./lib/command-component",
  "scripts": {
    "test": "mocha es5/test",
    "pretest": "npm run build && npm run clear-temp",
    "prepublish": "npm run build",
    "clear-temp": "rm -rf temp/ && mkdir temp",
    "build": "npm run build-lib && npm run build-test",
    "build-lib": "traceur --dir lib es5/lib --modules commonjs",
    "build-test": "traceur --dir test es5/test --modules commonjs"
  }
}
